FireWeapon15()
{
	if(tl1Flare == 1)
	{
		show t1_flare_1;
		sleep 200;
		hide t1_flare_1;
	}
	if(tl1Flare == 2)
	{
		show t1_flare_2;
		sleep 200;
		hide t1_flare_2;
	}
	if(tl1Flare == 3)
	{
		show t1_flare_3;
		sleep 200;
		hide t1_flare_3;
	}
	tl1Flare = tl1Flare + 1;
	if(tl1Flare>3)
	{
		tl1Flare = 1;
	}
}


[mk13torpedo] {
  name=Mk.13 22inch Torpedo launched from PT Boats;

  areaOfEffect=32;
  avoidFriendly=1;

  weapontype=LaserCannon;
  
  cegtag=torptrail;
  
  burnblow=0;
  canAttackGround=0;
  collideFriendly=1;
  [customParams] {
    damagetype=shapedcharge;
  }
  [damage] {
    default=150000;
  }
  edgeEffectiveness=0.5;
  explosionGenerator=custom:HE_XLarge;
  explosionSpeed=30;
  fireStarter=70;
  guidance=0;
  
  metalPerShot=0;
  model=Torpedo.S3O;
  noautorange=1;
  //NumBounce=5;
  range=2040;
  reloadTime=60;
  rendertype=1;

  soundHit=gen_explo_11;
  soundStart=watersplash;
  startVelocity=10;
  trajectoryHeight=0.25;
  tolerance=6000;
  turret=0;
  waterWeapon=1;
  weaponAcceleration=200;
  weaponVelocity=175;
}

[mk13torpedo_air] {

  name=Mk.13 22inch Torpedo dropped from aircraft;

  areaOfEffect=32;

  BounceRebound=0.1;
  BounceSlip=2;

  burnblow=0;


  canAttackGround=1;
  collideFriendly=1;
  CommandFire=1;

  [customParams] {
    damagetype=shapedcharge;
  }
  [damage] {
    default=150000;
  }
  edgeEffectiveness=0.5;

  explosionSpeed=30;
  explosionGenerator=custom:HE_XLarge;
  FireSubmersed=1;
  FixedLauncher=1;
  tracks=1;
  model=Bomb.S3O;
  NumBounce=1;
  //mygravity=0.06;
  range=1800;

  reloadTime=200;

  rendertype=1;

  soundHit=gen_explo_11;

  soundStart=bombrel;

  startVelocity=5;
  flighttime=10;
  tolerance=4000;

  turnRate=364;
  tracks=1;

  turret=1;
  WaterBounce=1;

  waterWeapon=1;
  weaponType=TorpedoLauncher;

  weaponAcceleration=5000;
  weaponVelocity=36;

}

